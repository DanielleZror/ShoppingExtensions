{
    "name": "Shopping Extensions",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [ "<all_urls>","tabs"],
    "browser_action":{
      "default_icon": "shoppingIcon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "manifest_version": 2
  }